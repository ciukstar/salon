:root

  --theme-accent: #6200ee
  --theme-on-accent: #FFFFFF

  --mdc-theme-primary: #FEDBD0
  --mdc-theme-on-primary: #442C2E
  
  --mdc-theme-secondary: #FEEAE6
  --mdc-theme-on-secondary: #442C2E
  
  --mdc-theme-background: #FFFFFF
  --mdc-theme-on-background: #442C2E

  --mdc-theme-surface: #FFFBFA
  --mdc-theme-on-surface: #442C2E
  
  --mdc-theme-text-primary-on-light: #442C2E
  --mdc-theme-text-secondary-on-light: #442C2E

  --mdc-theme-error: #C5032B
  --mdc-theme-on-error: #FFFBFA

  --mdc-text-button-label-text-color: #442C2E
  --mdc-outlined-button-label-text-color: #442C2E

  --mdc-switch-selected-handle-color: #FEDBD0
  --mdc-switch-unselected-handle-color: #FEEAE6

  --mdc-switch-selected-icon-color: #442C2E
  --mdc-switch-unselected-icon-color: #442C2E
  
  --mdc-switch-selected-hover-handle-color: #FEDBD0
  --mdc-switch-unselected-hover-handle-color: #FEEAE6

  --mdc-switch-selected-focus-handle-color: #FEDBD0
  --mdc-switch-unselected-focus-handle-color: #FEEAE6

  --mdc-switch-selected-pressed-handle-color: #FEDBD0
  --mdc-switch-unselected-pressed-handle-color: #FEEAE6

  --mdc-switch-selected-track-color: #FEEAE6
  --mdc-switch-unselected-track-color: #FEEAE6
  
  --mdc-switch-selected-track-color: #FEEAE6
  --mdc-switch-unselected-track-color: #FEEAE6
  
  --mdc-switch-selected-focus-track-color: #FEEAE6
  --mdc-switch-unselected-focus-track-color: #FEEAE6
  
  --mdc-switch-selected-hover-track-color: #FEEAE6
  --mdc-switch-unselected-hover-track-color: #FEEAE6
  
  --mdc-switch-selected-pressed-track-color: #FEEAE6
  --mdc-switch-unselected-pressed-track-color: #FEEAE6

  --mdc-checkbox-checked-color: var(--mdc-theme-primary)
  --mdc-checkbox-ink-color: var(--mdc-theme-on-primary)

html
  body
    margin: 0
    padding: 0
    #drawerTitle
      display: flex
      flex-direction: row
      justify-content: space-between
      align-items: center
      #drawerTitleStart
        display: flex
        flex-direction: row
        justify-content: flex-start
        align-items: center
        gap: 2rem
      #drawerTitleEnd
        color: gray
      #drawerTitleEnd:hover
        cursor: pointer