<header.mdc-top-app-bar.mdc-top-app-bar--fixed data-mdc-top-app-bar=MDCTopAppBar>
  <div.mdc-top-app-bar__row.mdc-theme--background>
    <section.mdc-top-app-bar__section.mdc-top-app-bar__section--align-start>
      <a.mdc-top-app-bar__navigation-icon.mdc-icon-button href=@{AdminR $ BusinessContactR bid} role=button title=_{MsgBack}>
        <span.mdc-icon-button__ripple>
        <span.mdc-icon-button__focus-ring>
        <i.material-symbols-outlined>arrow_back
      <span.mdc-top-app-bar__title.mdc-theme--text-primary-on-light>_{MsgContactUs}
        
<main.mdc-top-app-bar--fixed-adjust>
  <form method=post action=@{AdminR $ BusinessContactR bid} enctype=#{et} novalidate>
    ^{fw}
    <div.form-actions>
      <a.mdc-button href=@{AdminR $ BusinessContactR bid} role=button>
        <span.mdc-button__ripple>
        <span.mdc-button__label>_{MsgCancel}
      <button.mdc-button.mdc-button--raised type=submit>
        <span.mdc-button__ripple>
        <span.mdc-button__label>_{MsgSave}
